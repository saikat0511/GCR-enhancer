{
  "content_scripts": [
    {
      "matches": [ "https://classroom.google.com/*" ],
      "all_frames": true,
      "css": [ "styles/style.css" ],
      "js": [ "scripts/content.js" ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [ "storage" ],
  "manifest_version": 3,
  "name": "Enhancer for Classroomâ„¢",
  "version": "1.2.0",
  "description": "UI improvements for Google Classroom"
}
